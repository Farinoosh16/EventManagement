<SignupScreen>:
    canvas:
        Color:
            rgb: utils.get_color_from_hex("#9932CC")
        Rectangle:
            size: self.size
            pos: self.pos
    FloatLayout:
        ScrollView:
            GridLayout:
                cols:3
                size_hint_y: None
                height: self.minimum_height
                row_default_height:'100dp'
                row_force_default: True
                spacing: 20,20
                padding: 20,20
                Label:
                    size_hint: 1,1
                    pos_hint:{"top":1 , "left" : 1}
                ImageButton:
                    size_hint:.5,.2
                    pos_hint:{"top": .7 , "left" : 1}
                    source: "icons/finance.png "
                    on_release:
                        print("change sth")
                LabelButton:
                    size_hint: 1,0.2
                    pos_hint:{"top": .7 , "left" : 1}
                    text:"change sth"
                    on_release:
